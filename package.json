{
  "name": "baseline-widget",
  "type": "module",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "build:features": "bun run --filter '@baseline-widget/generator' build",
    "build:homepage": "bun run --filter '@baseline-widget/homepage' build",
    "dev:homepage": "bun run --filter '@baseline-widget/homepage' --elide-lines=0 dev"
  }
}
